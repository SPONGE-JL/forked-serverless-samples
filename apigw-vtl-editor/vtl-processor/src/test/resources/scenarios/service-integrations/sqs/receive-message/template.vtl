{
  #set($params = $util.parseJson($input.body))
  "QueueUrl": "$params.queueUrl",
  #if($params.maxNumberOfMessages)
  "MaxNumberOfMessages": $params.maxNumberOfMessages,
  #end
  #if($params.visibilityTimeout)
  "VisibilityTimeout": $params.visibilityTimeout,
  #end
  #if($params.waitTimeSeconds)
  "WaitTimeSeconds": $params.waitTimeSeconds,
  #end
  #if($params.attributeNames && $params.attributeNames.size() > 0)
  "AttributeNames": [
    #foreach($attrName in $params.attributeNames)
    "$attrName"#if($foreach.hasNext),#end
    #end
  ],
  #end
  #if($params.messageAttributeNames && $params.messageAttributeNames.size() > 0)
  "MessageAttributeNames": [
    #foreach($attrName in $params.messageAttributeNames)
    "$attrName"#if($foreach.hasNext),#end
    #end
  ]
  #end
}
